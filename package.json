{
  "name": "dsm23-next-payloadcms-template",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "fmt": "cross-env NODE_OPTIONS=--no-deprecation prettier . --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "fmt.check": "cross-env NODE_OPTIONS=--no-deprecation prettier . --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "cross-env NODE_OPTIONS=--no-deprecation jest"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.28.1",
    "@payloadcms/db-mongodb": "3.28.1",
    "@payloadcms/live-preview-react": "3.28.1",
    "@payloadcms/next": "3.28.1",
    "@payloadcms/payload-cloud": "3.28.1",
    "@payloadcms/plugin-form-builder": "3.28.1",
    "@payloadcms/plugin-nested-docs": "3.28.1",
    "@payloadcms/plugin-redirects": "3.28.1",
    "@payloadcms/plugin-search": "3.28.1",
    "@payloadcms/plugin-seo": "3.28.1",
    "@payloadcms/richtext-lexical": "3.28.1",
    "@payloadcms/ui": "3.28.1",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cross-env": "7.0.3",
    "geist": "1.3.1",
    "graphql": "16.10.0",
    "lucide-react": "0.479.0",
    "next": "15.2.2",
    "next-sitemap": "4.2.3",
    "payload": "3.28.1",
    "prism-react-renderer": "2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "sharp": "0.33.5",
    "tailwind-merge": "2.6.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/escape-html": "1.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "10.4.21",
    "commitizen": "4.3.1",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.22.0",
    "eslint-config-next": "15.2.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b",
  "engines": {
    "node": "18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
