    "test": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.test.local",
        ".env.local",
        ".env.test",
        ".env"
      ],
      "env": [
        "__NEXT_PRIVATE_ORIGIN",
        "DATABASE_URI",
        "PAYLOAD_SECRET",
        "NEXT_PUBLIC_SERVER_URL",
        "CRON_SECRET",
        "PREVIEW_SECRET",
        "NODE_ENV",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "MINIO_*",
        "SMTP_*"
      ]
    }
  }
}
