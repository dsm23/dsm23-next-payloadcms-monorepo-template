    "next": "15.5.6",
    "next-sitemap": "4.2.3",
    "nodemailer": "7.0.11",
    "payload": "3.64.0",
    "prism-react-renderer": "2.4.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
